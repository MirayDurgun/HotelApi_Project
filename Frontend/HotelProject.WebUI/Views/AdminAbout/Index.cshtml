@model List<ResultAboutDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered table-striped verticle-middle">
    <tr>
        <th>#</th>
        <th>Başlık 1</th>
        <th>Başlık 2</th>
        <th>Güncelle</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.AboutID</th>
            <td><span class="radient-1 btn-rounded">@item.Title1</span></td>
            <td>@item.Title2</td>
            <td><a href="/About/UpdateAbout/@item.AboutID" class="btn mb-1 btn-rounded gradient-3">Güncelle</a></td>
        </tr>

    }
</table>